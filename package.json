{
  "name": "fb-text2html",
  "version": "0.1.3",
  "description": "simple utility that transforms Facebook graph data: (text, tags) => html",
  "author": "Erik Hagreis",
  "keywords": [
    "facebook",
    "graph",
    "util",
    "text2html"
  ],
  "repository": "https://github.com/erikhagreis/fb-text2html",
  "license": "MIT",
  "main": "./lib/index.js",
  "scripts": {
    "precommit": "lint-staged",
    "prepublish": "babel src --out-dir lib --ignore spec.js"
  },
  "dependencies": {
    "autolinker": "^1.6.0",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-env": "^2.4.1",
    "husky": "^0.14.3",
    "jest": "^22.0.4",
    "lint-staged": "^6.0.0",
    "prettier": "^1.9.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --write",
      "git add"
    ]
  }
}
