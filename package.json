{
  "name": "fb-text2html",
  "version": "2.0.0",
  "description": "simple utility that transforms Facebook graph data: (text, tags) => html",
  "author": "Erik Hagreis",
  "keywords": [
    "facebook",
    "graph",
    "util",
    "text2html"
  ],
  "repository": "https://github.com/erikhagreis/fb-text2html",
  "license": "MIT",
  "main": "./lib/index.js",
  "scripts": {
    "precommit": "lint-staged",
    "build": "webpack",
    "lib": "babel src --out-dir lib --ignore spec.js,umd.js",
    "prepublish": "npm run build && npm run lib"
  },
  "dependencies": {
    "autolinker": "^1.6.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-lodash": "^3.3.2",
    "lodash": "^4.17.4",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-env": "^2.4.1",
    "husky": "^0.14.3",
    "jest": "^22.0.4",
    "lint-staged": "^6.0.0",
    "prettier": "^1.9.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --write",
      "git add"
    ]
  }
}
